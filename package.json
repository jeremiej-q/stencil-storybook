{
  "name": "stencil-storybook",
  "version": "1.0.0",
  "description": "Simple application to learn stencil and storybook",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-storybook/stencil-storybook.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "storybook": "start-storybook -s ./www",
    "all": "concurrently \"npm run build\" \"npm run storybook\"",
    "generate": "plop"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@stencil/core": "1.2.2",
    "@storybook/addon-a11y": "^5.0.11",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/html": "^5.1.9",
    "@types/jest": "^24.0.16",
    "@types/storybook__addon-knobs": "^5.0.3",
    "@types/storybook__html": "^5.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "case": "^1.6.2",
    "concurrently": "^4.1.0",
    "glob": "^7.1.4",
    "husky": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "plop": "^2.4.0",
    "storybook": "^1.0.0",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanmugapriyaEK/stencil-storybook.git"
  },
  "author": "Shanmuga Priya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shanmugapriyaEK/stencil-storybook/issues"
  },
  "homepage": "https://github.com/shanmugapriyaEK/stencil-storybook#readme"
}
